<body>
    <div class="wrapper">
        <div class="main-header">
            <%- include('../layouts/index.ejs') %>
                <div class="main-panel">
                    <div class="content">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="card">
                                        <div class="card-header">
                                            <div class="card-title">Thông tin thành viên</div>
                                        </div>
                                        <div class="card-body">
                                            <table class="table table-hover">
                                                <thead>
                                                    <tr>
                                                        <th scope="col" style="display: none;">#</th>
                                                        <th scope="col">Mã dân cư</th>
                                                        <th scope="col" style="display: none;">ID căn hộ</th>
                                                        <th scope="col">Họ và Tên</th>
                                                        <th scope="col">Ngày sinh</th>
                                                        <th scope="col">Giới tính</th>
                                                        <th scope="col">Quê hương</th>
                                                        <th scope="col">Số điện thoại</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <% citizen.forEach(function(citizen) { %>
                                                        <tr>
                                                            <td style="display: none;">
                                                                <%= citizen.id %>
                                                            </td>
                                                            <td>
                                                                <%= citizen.CitizenID %>
                                                            </td>
                                                            <td style="display: none;">
                                                                <%= citizen.ApartID %>
                                                            </td>
                                                            <td>
                                                                <%= citizen.Name %>
                                                            </td>
                                                            <td>
                                                                <%= citizen.BirthDay.toLocaleDateString() %>
                                                            </td>
                                                            <td>
                                                                <%= citizen.Gender %>
                                                            </td>
                                                            <td>
                                                                <%= citizen.Hometown %>
                                                            </td>
                                                            <td>
                                                                <%= citizen.Phone %>
                                                            </td>
                                                        </tr>
                                                        <% }); %>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                    <input class="btn btn-secondary" type="button" value="Quay lại"
                                        onclick="history.back()" />
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
        </div>
    </div>
</body>

</html>